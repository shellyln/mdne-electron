{
    "extends": ["eslint:recommended"],
    "plugins": [],
    "parser": "babel-eslint",
    "parserOptions": {
        "ecmaVersion": 9,
        "sourceType": "module"
    },
    "env": {
        "node": true,
        "es6": true
    },
    "globals": {
        "SharedArrayBuffer": true,
        "Atomics": true,
        "window": true,
        "document": true,
        "M": true,
        "ace": true,
        "React": true,
        "ReactDOM": true,
        "liyad": true,
        "lsx": true,
        "alert": true,
        "confirm": true,
        "mdneApi": true
    },
    "rules": {
        "no-console": "warn",
        "no-unused-vars": "warn",
        "no-empty": "warn",
        "no-inner-declarations": 0
    }
}
